  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch UPG Tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/main",
      "args": ["upg_test_cmd_pack.blk"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build project",
      "miDebuggerPath": "/usr/bin/gdb"
    }
    {
      "name": "Launch UPG Benchmark",
